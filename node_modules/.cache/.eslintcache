                <button className=\"btn btn-buffer btn-dark help-btn\" onClick={(e) => {\n                            dispatch(settingsView(true)); \n                            dispatch(toggleView(false, username));\n                        }\n                    }>\n                            Settings\n                        </button>\n                    </>\n                }\n\n                {\n                    loginState === LOGIN_STATE.LOGGED_IN &&\n                    <>\n                        <div className=\"navbar-brand\" id=\"seperator\">|</div>\n                    </>\n                }\n                    \n                {\n                    loginState === LOGIN_STATE.LOGGED_IN &&\n                    <button className=\"btn btn-dark float-end\" onClick={() => dispatch(logout())}>\n                        Log Out\n                    </button> \n                }\n            </div>\n        </nav>\n    )\n}\n\nexport default Header","/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/Onboarding.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/client.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/AddPost.js",["265"],"import {useState} from \"react\";\nimport {useDispatch, useSelector} from \"react-redux\";\nimport { viewPost, addPost, getPosts, uploadPic } from \"../redux/actions\";\n\nconst AddPost = () => {\n    const [postCaption, setPostCaption] = useState(\"\");\n    const [postPictureURL, setPictureURL] = useState(\"\");\n    const [postTags, setPostTags] = useState({});\n    const [successMsg, setSuccessMsg] = useState(false);\n\n    const username = useSelector(state => state.user.username);\n    const url = useSelector(state => state.url);\n\n    const updateTags = tagsList => {\n        // separate tags by whitespace\n        setPostTags(tagsList.split(/\\s+/));\n    }\n\n    const uploadPicture = (pic) => {\n        console.log(\"picture: \", pic)\n        dispatch(uploadPic(pic, setPictureURL));\n        //setPictureURL(pic)\n        \n    }\n\n    const dispatch = useDispatch();\n    return (\n        <div className=\"card add-post\">\n            <div className=\"card-body\">\n                {\n                    successMsg && \n                    <div className=\"alert alert-success\">You've successfully added your post!</div>\n                }\n                <b>\n                    <label htmlFor=\"post-title\">Post Caption:</label>\n                </b>\n                <div className=\"input-group mb-3\">\n                    <textarea\n                        value={postCaption}\n                        type=\"textarea\" \n                        lines=\"5\"\n                        placeholder=\"Your post caption\" \n                        className=\"form-control\" \n                        aria-label=\"Post Caption\" \n                        onChange={\n                            (e) => {\n                            setPostCaption(e.target.value)\n                            }\n                        }\n                        required>\n                    </textarea>\n                </div>\n                <br></br>\n                <div className=\"mb-3\">\n                    <div class=\"input-group\">\n                        <input type=\"file\" class=\"form-control\" id=\"inputGroupFile04\" aria-describedby=\"inputGroupFileAddon04\" aria-label=\"Upload\" onChange={(e) => uploadPicture(e.target.files[0])}/>\n                        <button class=\"btn btn-outline-dark\" type=\"button\" id=\"inputGroupFileAddon04\">Upload</button>\n                    </div>\n                </div>\n                {/* <div className=\"mb-3\">\n                    <b>\n                        <label htmlFor=\"post-msg\">Post Picture:</label>\n                    </b>\n                    <input\n                        value={postPictureURL}\n                        className=\"form-control\" \n                        placeholder=\"Please copy the URL of the picture you would like to see!\"\n                        id=\"postTextArea\" \n                        aria-label=\"Picture URL\" \n                        rows=\"3\"\n                        onChange={(e) => {\n                            setPictureURL(e.target.value)\n                        }\n                    }>\n                    </input>\n                </div> */}\n                <br></br>\n                <div className= \"mb-3\">\n                    <b>\n                        <label htmlFor=\"tag-msg\">Tags:</label>\n                    </b>\n                    <input \n                        type=\"text\" \n                        className=\"form-control\" \n                        placeholder=\"Tags (Every tag starts with a # and multiple tags can be separated by whitespace)\" \n                        aria-label=\"Tags\" \n                        aria-describedby=\"basic-addon1\" \n                        onChange={e => updateTags(e.target.value)}\n                    />\n                </div>\n                <br></br>\n                <div class=\"d-grid gap-2 col-6 mx-auto\">\n                    <button \n                        className=\"btn custom-btn-style\" \n                        aria-label = \"buttonSubmit\"\n                        type=\"button\" \n                        id=\"buttonSubmit\"\n                        onClick={() => {\n                            \n                            dispatch(addPost(postCaption, postPictureURL, postTags, username));\n                            dispatch(getPosts());\n                            setPostCaption(\"\");\n                            setPictureURL(\"\");\n                            setSuccessMsg(true);\n                            setTimeout(() => {\n                                setSuccessMsg(false);\n                                dispatch(viewPost());\n                            }, 1000)\n                        }\n                    }>Post</button>\n                </div>\n                <br></br>\n                <div class=\"d-grid gap-2 col-6 mx-auto\">\n                    <button \n                        className=\"btn btn-outline-dark\" \n                        aria-label = \"buttonSubmit\"\n                        type=\"button\" \n                        id=\"buttonSubmit\"\n                        onClick={() => {\n                            dispatch(viewPost());\n                            setPostCaption(\"\");\n                            setPictureURL(\"\");\n                        }\n                    }>Cancel Post</button>\n                </div>\n            </div>\n        </div>\n    )\n}\nexport default AddPost;","/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/Sidebar.js",["266","267","268"],"import Chat from \"./Chat\";\nimport Login from \"./Login\";\nimport ProfileBubble from \"./ProfileBubble\";\nimport {useSelector} from \"react-redux\";\nimport { LOGIN_STATE } from \"../redux/stateConstants\";\nimport InvalidLogin from \"./InvalidLogin\";\nimport CreateUser from \"./CreateUser\";\nimport { useState } from \"react\";\n\nconst Sidebar = () => {\n    const usersOnline = useSelector(state => state.onlineUsers);\n    const users = useSelector(state => state.users)\n    const usersOnlineNames = Object.keys(usersOnline);\n    const loginState = useSelector(state => state.loginState);\n    const createdNew = useSelector(state => state.createdNewUser);\n    const [create, setCreate] = useState(false);\n    const [userPicsMap, setUserPicsMap] = useState({});\n    const picture = useSelector(state => state.user.profilePicture);\n\n    console.log(\"users: \", users)\n    console.log(\"usersOnline: \", usersOnline)\n    console.log(\"usersOnlineNames: \", usersOnlineNames)\n\n    for (let each of Object.keys(users)) {\n            userPicsMap[users[each].username] = users[each].profilePicture\n        }\n    \n    console.log(\"userPicsMap: \", userPicsMap)\n\n    return (\n        <div className=\"container sidebar-profile-container container-no-margins\">\n            {\n            ((loginState === LOGIN_STATE.LOGGED_OUT) && (create)) ?\n                    <CreateUser setCreate={setCreate}/>:\n                 (loginState === LOGIN_STATE.LOGGED_OUT) ?\n                    <Login setCreate={setCreate} />:\n                     \"\"\n            }\n            \n            {\n                loginState === LOGIN_STATE.LOGGED_IN && \n                <>\n                <aside className=\"profile-bubble-column pe-2\">\n                    {usersOnlineNames.map((username, index) => (\n                        \n                        <ProfileBubble key={index} userpicture={userPicsMap[username]} username={username} big={false} />\n                    ))}\n                </aside>\n                <Chat/>\n                </>\n             }\n            {loginState === LOGIN_STATE.INVALID_LOGIN && <InvalidLogin />}\n        </div>\n    )\n}\n\nexport default Sidebar","/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/Posts.js",["269","270","271","272","273","274"],"import { useSelector, useDispatch } from \"react-redux\";\nimport Post from \"./Post\";\nimport Settings from \"./Settings\";\nimport { ADD_POST_PAGE, VIEW_POST } from \"../redux/actionConstants\";\nimport AddPost from \"./AddPost\";\nimport { LOGIN_STATE } from \"../redux/stateConstants\";\nimport { logout, addPostPage, completeOnboarding } from \"../redux/actions\";\nimport Profile from \"./Profile\";\n\nconst Posts = () => {\n    const posts = useSelector(state => state.posts);\n    const viewState = useSelector(state => state.viewState);\n    const loginState = useSelector(state => state.loginState);\n    const profileView = useSelector(state => state.profileView);\n    const settingsView = useSelector(state => state.settingsView);\n    const isNewUser = useSelector(state => state.isNewUser);\n    const userId = useSelector(state => state.user.userId);\n    const username = useSelector(state => state.user.username);\n    const dispatch = useDispatch();\n    const ids = Object.keys(posts);\n    \n    // sort the post ids by time\n    const rearrangedIds = ids.map(id => {\n        const date = posts[id].datePosted;\n        return [date, id]\n    }).sort((elem1, elem2) => {\n        return elem1[0] > elem2[0] ? 1 : elem1[0] < elem2[0] ? -1 : 0;\n    }).reverse()\n\n    return (\n\n        <div className=\"container container-no-margins posts\">\n            \n            \n            {(profileView) ? <Profile posts={rearrangedIds}/>:\n            (settingsView) ? <Settings /> :\n            <>\n            <div className=\"add-post-header\">\n            {\n                    (loginState === LOGIN_STATE.LOGGED_IN && viewState === VIEW_POST && (isNewUser !== undefined && !isNewUser)) ? \n                    <>\n                        <button className=\"btn add-button\" onClick={() => dispatch(addPostPage())}>\n                            +\n                        </button>\n                    </>:\"\"\n                }\n            </div>\n            <div className=\"row row-cols-3 justify-content-center\">\n            {rearrangedIds.map((element, index)=>{\n                return (\n                    <div className=\"cols justify-content-center\">\n                        <Post key={index} id={element[1]} />\n                    </div>\n                )\n            })}\n            </div>\n            </>}\n        </div>\n    )\n}\n\nexport default Posts","/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/SlideNavigators.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/Slide.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/data/fbConfig.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/ProfileBubble.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/components/Login.js",["275"],"import {useState} from \"react\";\nimport {useDispatch, useSelector} from \"react-redux\";\nimport {loginSuccess, loginFail} from \"../redux/actions\";\nimport { joinChat } from \"../client\";\nimport CreateUser from \"./CreateUser\";\n\nconst Login = (props) => {\n\n    const [username, setUsername] = useState(\"\"); \n    const [password, setPassword] = useState(\"\");\n    const dispatch = useDispatch();\n    const users = useSelector(state => state.users)\n\n    const checkLogin = (username, password) => {\n        for (let user in users) { \n            if (username.toLowerCase() === users[user].username.toLowerCase() \n                && password.toLowerCase() === users[user].password.toLowerCase()) {\n                joinChat(users[user].username);\n                return users[user];\n            }\n        }\n        return false;\n    }\n\n    const handleLogin = () => {\n        const user = checkLogin(username, password);\n        if (user !== false) {\n            dispatch(loginSuccess(user))\n        }\n        else {\n            dispatch(loginFail())\n        }\n        setUsername(\"\");\n        setPassword(\"\");\n    }\n\n    const handleKeyPress = event => {\n        if (event.keyCode === 13) {\n            event.preventDefault();\n            handleLogin();\n        }\n    }\n\n    return (\n        <>\n            <div className=\"card align-self-center border-0\">\n                <div className=\"card-body custom-background-color\">\n                    <h5 className=\"card-title text-aligner text-center\">Snap a pic and want to share it with your friends? <br></br> </h5>\n                    <div className=\"row my-4\">\n                        <div className=\"col-12 col-sm-2\">\n                            <label htmlFor=\"username\" className=\"form-label\">Username:</label>\n                        </div>\n                        <div className