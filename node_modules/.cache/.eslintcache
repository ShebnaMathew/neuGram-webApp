bobby_lupo_lisa_lam_miranda_tran_shebna_mathew_finalproject/src/components/InvalidLogin.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/bobby_lupo_lisa_lam_miranda_tran_shebna_mathew_finalproject/src/components/Onboarding.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/bobby_lupo_lisa_lam_miranda_tran_shebna_mathew_finalproject/src/components/Slide.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/bobby_lupo_lisa_lam_miranda_tran_shebna_mathew_finalproject/src/components/SlideNavigators.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/index.js",[],["256","257"],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/reportWebVitals.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/redux/store.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/views/App.js",["258"],"import {useDispatch, useSelector} from \"react-redux\"; // Automatically passes the store to all child components\nimport { getPosts, getUsers } from \"../redux/actions\";\nimport { useEffect } from \"react\";\nimport MainApp from \"../components/MainApp\";\nimport Header from \"../components/Header\";\nimport Onboarding from \"../components/Onboarding\";\n\n\nconst App = () =>  {\n\n    const dispatch = useDispatch();\n    const isNewUser = useSelector(state => state.isNewUser);\n\n    useEffect( () => {\n      dispatch(getPosts());\n      dispatch(getUsers());\n    }, [])\n\n    return (\n\n      <> \n        <Header/>\n        {\n          (isNewUser === undefined || isNewUser === false) ?\n          <MainApp/>\n          :\n          <Onboarding />\n        }\n      </>\n    )\n}\nexport default App;\n","/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/redux/stateConstants.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/redux/actionConstants.js",[],"/Users/shebthebat/Documents/NEU/Sem 5/CS 7580/neuGram-webApp/src/redux/actions.js",[],"/Users/shebthebat/Documents/NEU/S